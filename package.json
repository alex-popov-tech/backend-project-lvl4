{
  "name": "backend-project-lvl4",
  "version": "1.0.0",
  "description": "hexlet nodejs backend project lvl 4",
  "scripts": {
    "clean": "del dist",
    "build:client": "NODE_ENV=production webpack -p",
    "build:server": "NODE_ENV=production babel server --out-dir dist/server && cp -r server/views dist/server/views && cp knexfile.js dist/knexfile.js",
    "migrate": "NODE_ENV=production npx knex migrate:latest",
    "build": "npm run clean && npm run build:client && npm run build:server && npm run migrate",
    "start": "NODE_ENV=production node dist/server/bin/index.js",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "__tests__/.*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-popov-tech/backend-project-lvl4.git"
  },
  "keywords": [
    "hexlet",
    "nodejs",
    "backend"
  ],
  "author": "Alexander Popov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alex-popov-tech/backend-project-lvl4/issues"
  },
  "homepage": "https://github.com/alex-popov-tech/backend-project-lvl4#readme",

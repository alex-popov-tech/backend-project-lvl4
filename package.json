    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.13.0",
    "fastify-error-page": "^4.0.0",
    "fastify-errors-properties": "^0.7.0",
    "fastify-formbody": "^5.0.0",
    "fastify-method-override": "^1.5.4",
    "fastify-objectionjs": "^0.3.0",
    "fastify-passport": "^0.4.2",
    "fastify-secure-session": "^2.3.1",
    "fastify-static": "^4.0.1",
    "fastify-webpack-hmr": "^3.0.0",
    "jquery": "^3.6.0",
    "knex": "^0.95.1",
    "lodash": "^4.17.21",
    "objection": "^2.2.14",
    "objection-password": "^3.0.0",
    "objection-unique": "^1.2.1",
    "pino-pretty": "^4.7.1",
    "point-of-view": "^4.14.0",
    "precss": "^4.0.0",
    "pug": "^3.0.2",
    "rollbar": "^2.21.0",
    "sqlite3": "^5.0.2"
